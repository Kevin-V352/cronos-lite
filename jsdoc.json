{
  "opts": {
    "encoding": "utf8",
    "readme": "./README.md",
    "destination": "./docs",
    "recurse": true,
    "verbose": true,
    "template": "./node_modules/clean-jsdoc-theme",
    "theme_opts": {
      "default_theme": "dark",
      "base_url": "./docs",
      "favicon": "/src/assets/icons/favicon.ico",
      "homepageTitle": "CRONOS LITE | DOCS v1.0",
      "title": "CRONOS LITE | DOCS v1.0",
      "search": true,
      "menu": [
        {
          "title": "GitHub",
          "link": "https://github.com/Kevin-V352/cronos-lite",
          "target": "_blank"
        },
        {
          "title": "Deploy",
          "link": "https://cronos-lite-kv357.netlify.app/",
          "target": "_blank"
        }
      ]
    }
  },
  "tags": {
    "allowUnknownTags": true
  },
  "source": {
    "include": ["./src"],
    "includePattern": "\\.(js|ts)$",
    "exlude": "node_modules/|docs"
  },
  "plugins": [
    "plugins/markdown",
    "node_modules/better-docs/typescript"
  ],
  "markdown": {
    "hardwrap": false,
    "idInHeadings": true
  }
}